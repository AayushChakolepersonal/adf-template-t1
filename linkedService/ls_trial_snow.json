{
	"name": "ls_trial_snow",
	"properties": {
		"parameters": {
			"snow_account_name": {
				"type": "string"
			},
			"snow_db_name": {
				"type": "string"
			},
			"snow_ware_name": {
				"type": "string"
			},
			"snow_user_name": {
				"type": "string"
			},
			"ls_snow_kv_name": {
				"type": "string"
			},
			"ls_snow_secret_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Snowflake",
		"typeProperties": {
			"authenticationType": "Basic",
			"connectionString": "jdbc:snowflake://@{linkedService().snow_account_name}.snowflakecomputing.com/?user=@{linkedService().snow_user_name}&db=@{linkedService().snow_db_name}&warehouse=@{linkedService().snow_ware_name}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_trial_kv",
					"type": "LinkedServiceReference",
					"parameters": {
						"keyvault_name": {
							"value": "@linkedService().ls_snow_kv_name",
							"type": "Expression"
						}
					}
				},
				"secretName": {
					"value": "@linkedService().ls_snow_secret_name",
					"type": "Expression"
				}
			}
		}
	}
}